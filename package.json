{
  "name": "gilded-rose-kata",
  "version": "1.0.0",
  "description": "Gilded Rose kata in TypeScript",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "pretest": "rimraf app/**/*.js test/**/*.js",
    "test": "jest --config ./jest.config.json"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node": "~7.0.18",
    "jest": "^29.3.1",
    "nyc": "~11.0.3",
    "rimraf": "~2.5.2",
    "source-map-support": "0.5.9",
    "ts-jest": "^23.10.5",
    "ts-node": "~3.1.0",
    "typescript": "~3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "dependencies": {
    "@types/jest": "^24.0.5",
    "lodash": "^4.17.11"
  }
}
